{
    "name": "orvital/laravel-uid",
    "description": "Unique identifiers for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jona Goldman",
            "email": "jonagoldman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/uid": "^6.0",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Orvital\\Uid\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orvital\\Uid\\UidServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
